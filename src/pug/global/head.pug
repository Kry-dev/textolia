mixin head(title)
	head
		meta(charset="utf-8")
		title= title
		meta(name="keywords" content="keywords")
		meta(name="description" content="description")
		//favicon
		//-mixin
		mixin favicon(rel, sizes, href, type)
			link(rel= rel, sizes= sizes, href= href, type= type)
